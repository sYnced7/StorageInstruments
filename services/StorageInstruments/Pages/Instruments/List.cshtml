@page
@model StorageInstruments.Pages.Instruments.ListModel
@{
    ViewData["Title"] = "List";
}

<h1>List</h1>
<form action="/Instruments/List" method="get">
      <div class="form-group">
          <div class="input-group">
              <input type="search" class="form-control" asp-for="SearchTerm"/>
              <span class="input-group-btn">
                  <button class="btn btn-default">
                      <i class="fa fa-search"></i>
                  </button>
              </span>
          </div>
      </div>
</form>
@foreach (var item in Model.Instruments)
{
    <partial name="_Summary" model="item" />		 
}

<a asp-page="./Edit" class="btn btn-primary">Add New</a>


